{
  "name": "monopoly",
  "version": "1.0.0",
  "scripts": {
    "init": "npm install && npm run build:dev:vendors",
    "dev": "better-npm-run dev",
    "start:dev": "NODE_PATH=./dist CFG_DIR=$(pwd)/dist/configs node dist/server/index.js",
    "watch:ssr": "better-npm-run watch:ssr",
    "build:configs": "tsc --project configs",
    "build:server": "tsc --project server",
    "build:dev:vendors": "NODE_ENV=development webpack --mode=development --config webpack/config/vendors.config.ts"
  },
  "betterScripts": {
    "dev": {
      "command": "npm-run-all build:configs build:server && npm-run-all --parallel --print-name watch:ssr start:dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:production": {
      "command": "npm-run-all build:configs build:server build:production:vendors build:ssr build:production:client",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:production:vendors": {
      "command": "webpack --mode=production --config webpack/config/vendors.config.ts",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:production:client": {
      "command": "webpack --mode=production --config webpack/config/client.config.ts",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "watch:ssr": {
      "command": "webpack --watch --config webpack/config/ssr.config.ts",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/10-gotham-city/monopoly.git"
  },
